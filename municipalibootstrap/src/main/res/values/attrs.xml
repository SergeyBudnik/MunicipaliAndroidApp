<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MunicipaliLoadableImageView">
        <attr name="widthRatio" format="integer"/>
        <attr name="heightRatio" format="integer"/>
        <attr name="ratioBasis" format="enum">
            <enum name="width" value="1"/>
            <enum name="height" value="2"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="MunicipaliButtonView">
        <attr name="text" format="string"/>
        <attr name="enabled" format="boolean"/>
        <attr name="type" format="enum">
            <enum name="primary" value="1"/>
            <enum name="secondary" value="2"/>
        </attr>
    </declare-styleable>
</resources>